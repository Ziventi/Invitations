{
  "name": "ziventi",
  "private": true,
  "version": "1.0.0",
  "stylelint": {
    "extends": "@zzavidd/stylelint-config"
  },
  "prettier": "@zzavidd/prettier-config",
  "scripts": {
    "bootstrap": "lerna exec npm install --parallel",
    "build": "node scripts/build.js",
    "check": "tsc --noEmit",
    "cli": "node scripts/cli.js",
    "clean": "node scripts/clean.js",
    "dev": "node scripts/dev.js",
    "dev:server": "nodemon --watch server/*.ts server/index.ts",
    "watch": "node scripts/watch.js",
    "test": "jest --silent --runInBand",
    "debug:cli": "node --inspect scripts/cli.js",
    "eslint": "eslint --ext .ts .",
    "stylelint": "stylelint **/*.scss"
  },
  "dependencies": {
    "@ziventi/utils": "file:utils",
    "dotenv": "^10.0.0",
    "express": "^4.17.2"
